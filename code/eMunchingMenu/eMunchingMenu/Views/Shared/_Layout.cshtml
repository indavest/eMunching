<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="utf-8" />
        <title>@ViewBag.Title</title>
        <link href="@System.Web.Optimization.BundleTable.Bundles.ResolveBundleUrl("~/Content")"  rel="stylesheet" type="text/css" />
        <link href="@System.Web.Optimization.BundleTable.Bundles.ResolveBundleUrl("~/Content/themes/base")" rel="stylesheet" type="text/css" />
        <script src="@System.Web.Optimization.BundleTable.Bundles.ResolveBundleUrl("~/Scripts")" type="text/javascript"> </script>

        <!--Include JQuery and JQuery UI-->
        <script src="@Url.Content("~/Scripts/jquery-1.8.2.min.js")" type="text/javascript"></script>
        <link href="@Url.Content("~/Content/themes/base/jquery.ui.all.css")" type="text/css" rel="stylesheet" />
        <script src="@Url.Content("~/Scripts/jquery-ui-1.9.0.min.js")" type="text/javascript"></script>

    </head>
    <body>
        @RenderBody()
    </body>
</html>
