@model eMunchingMenu.Models.EventModel

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>
@{
    foreach(var e in Model.Events)
    {        
        <div>
            @e.Name;
        </div>
        <div id="@e.EventId" onload="">
            @e.Description;
        </div>
        
    }
}